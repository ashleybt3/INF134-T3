 => this._updateLatestValue(obj, value));\n        }\n        _selectStrategy(obj) {\n            if (Object(_angular_core__WEBPACK_IMPORTED_MODULE_0__[/* ɵisPromise */ \"rb\"])(obj)) {\n                return _promiseStrategy;\n            }\n            if (Object(_angular_core__WEBPACK_IMPORTED_MODULE_0__[/* ɵisSubscribable */ \"sb\"])(obj)) {\n                return _subscribableStrategy;\n            }\n            throw invalidPipeArgumentError(AsyncPipe, obj);\n        }\n        _dispose() {\n            this._strategy.dispose(this._subscription);\n            this._latestValue = null;\n            this._subscription = null;\n            this._obj = null;\n        }\n        _updateLatestValue(async, value) {\n            if (async === this._obj) {\n                this._latestValue = value;\n                this._ref.markForCheck();\n            }\n        }\n    }\n    AsyncPipe.ɵfac = function AsyncPipe_Factory(t) { return new (t || AsyncPipe)(_angular_core__WEBPACK_IMPORTED_MODULE_0__[/* ɵɵinjectPipeChangeDetectorRef */ \"Tb\"]()); };\n    AsyncPipe.ɵpipe = /*@__PURE__*/ _angular_core__WEBPACK_IMPORTED_MODULE_0__[/* ɵɵdefinePipe */ \"Jb\"]({ name: \"async\", type: AsyncPipe, pure: false });\n    return AsyncPipe;\n})();\nlet LowerCasePipe = /*@__PURE__*/ (() => {\n    class LowerCasePipe {\n        transform(value) {\n            if (value == null)\n                return null;\n            if (typeof value !== 'string') {\n                throw invalidPipeArgumentError(LowerCasePipe, value);\n            }\n            return value.toLowerCase();\n        }\n    }\n    LowerCasePipe.ɵfac = function LowerCasePipe_Factory(t) { return new (t || LowerCasePipe)(); };\n    LowerCasePipe.ɵpipe = /*@__PURE__*/ _angular_core__WEBPACK_IMPORTED_MODULE_0__[/* ɵɵdefinePipe */ \"Jb\"]({ name: \"lowercase\", type: LowerCasePipe, pure: true });\n    return LowerCasePipe;\n})();\n//\n// Regex below matches any Unicode word and compatible with ES5. In ES2018 the same res